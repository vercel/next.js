{
  "extends": "../../tsconfig-tsec.json",
  "compilerOptions": {
    "strict": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ES2018",
    "moduleResolution": "node"
  },
  "exclude": [
    "dist",
    "./*.d.ts",
    "future/*.d.ts",
    "image-types/global.d.ts",
    "compat/*.d.ts",
    "legacy/*.d.ts",
    "types/compiled.d.ts",
    "navigation-types/*.d.ts",
    "navigation-types/compat/*.d.ts",
    "experimental/**/*.d.ts"
  ]
}
