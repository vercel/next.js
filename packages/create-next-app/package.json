    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.0",
    "tar": "4.4.10",
    "typescript": "3.8.3",
    "update-check": "1.5.4",
