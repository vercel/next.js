{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["NEXT_CI_RUNNER"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"]
    },
    "storybook": {},
    "build-storybook": {
      "dependsOn": ["^build-storybook"],
      "outputs": ["storybook-static/**"]
    },
    "test-storybook": {
      "dependsOn": ["^test-storybook"]
    },
    "typescript": {},
    "//#typescript": {},
    "//#get-test-timings": {
      "inputs": ["run-tests.js"],
      "outputs": ["test-timings.json"]
    }
  },
  "ui": "tui"
}
