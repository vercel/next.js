  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "toolbox": "react-toolbox-themr"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "next": "latest",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-toolbox": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "react-toolbox-themr": "^1.0.2"
  },
  "reactToolbox": {
    "include": [
      "BUTTON",
      "DATE_PICKER"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
