        "width": {
          "label": "Width",
          "unique": false,
          "inputType": "number"
        },
        "height": {
          "label": "Height",
          "unique": false,
          "inputType": "number"
        },
        "source": {
          "label": "Source",
          "unique": false,
          "options": [
            "disk",
            "unsplash"
          ],
          "inputType": "select"
        },
        "fileName": {
          "label": "File name",
          "unique": false,
          "inputType": "text",
          "isTitlePart": true
        },
        "mimeType": {
          "label": "MIME type",
          "unique": false,
          "inputType": "text"
        },
        "variants": {
          "items": {
            "order": [
              "name",
