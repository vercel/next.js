                    "inputType": "number"
                  },
                  "width": {
                    "label": "Width",
                    "unique": false,
                    "inputType": "number"
                  },
                  "bottom": {
                    "label": "Bottom",
                    "unique": false,
                    "inputType": "number"
                  },
                  "height": {
                    "label": "Height",
                    "unique": false,
                    "inputType": "number"
                  }
                }
              }
            }
          },
          "label": "Variants",
          "unique": false,
          "helpText": "",
          "inputType": "object"
        },
        "extension": {
          "label": "Extension",
