    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-es2015": "6.24.1",
    "benchmark": "2.1.4",
    "cheerio": "0.22.0",
    "chromedriver": "2.32.3",
    "clone": "2.1.1",
    "coveralls": "2.13.1",
    "cross-env": "5.0.5",
    "express": "4.15.5",
    "fkill": "5.1.0",
    "get-port": "3.2.0",
    "husky": "0.14.3",
