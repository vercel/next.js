    "mkdirp-then": "1.2.0",
    "mv": "2.1.1",
    "mz": "2.7.0",
    "path-to-regexp": "2.1.0",
    "pkg-up": "2.0.0",
    "prop-types": "15.6.0",
    "prop-types-exact": "1.1.1",
    "react-hot-loader": "4.0.0-beta.23",
